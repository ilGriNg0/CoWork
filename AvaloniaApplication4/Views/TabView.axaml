<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="clr-namespace:AvaloniaApplication4.ViewModels"
             x:Class="AvaloniaApplication4.Views.TabView"
             x:DataType="vm:TabViewModel">
    <TabControl>
           <TabItem  Header="Бронирование на час"  FontSize="15" HorizontalAlignment="Center">
               <Grid ColumnDefinitions="*,*" RowDefinitions="*,*" ShowGridLines="True">
                   <Border Grid.Row="0" Grid.Column="0" Background="Gray" CornerRadius="15">
                       <TextBlock Text="200$$"></TextBlock>
                   </Border>
			   </Grid>
           </TabItem>
           <TabItem Header="Броинрование переговорной" FontSize="15" HorizontalAlignment="Center">
			   <Grid ColumnDefinitions="*,*" RowDefinitions="*,*" ShowGridLines="True">
                   <Border Grid.Row="0" Grid.Column="1" Background="Gray" CornerRadius="15">
                       <TextBlock Text="1200$$"></TextBlock>
                   </Border>
               </Grid>
           </TabItem>
    </TabControl>
</UserControl>
